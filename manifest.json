{
    "manifest_version": 3,
    "name": "AnyOne Network",
    "version": "0.1",
    "description": "Browse the internet via the AnyOne Network",
    "permissions": [
      "proxy",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon-on.png",
        "48": "icon-on.png",
        "128": "icon-on.png"
      }
    },
    "icons": {
      "128": "icon-on.png"
    }
  }
  